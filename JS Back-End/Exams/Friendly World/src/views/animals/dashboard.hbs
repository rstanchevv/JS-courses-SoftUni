<main class="dashboard">
    <section id="dashboard">
        <h2>Animals in Need</h2>
        <ul class="card-wrapper">
            {{#each animals}}
            {{>animal}}
            {{else}}
            <div id="no-post">
                <h2>There are no animals in need now! </h2>
            </div>
            {{/each}}
        </ul>
    </section>
</main>